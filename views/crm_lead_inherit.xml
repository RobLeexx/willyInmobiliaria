<odoo>
  <data>
    <record id="mudanzas_crm_crm_lead_form_inherit" model="ir.ui.view">
      <field name="name">mudanzas.crm.lead.form.inherit</field>
      <field name="model">crm.lead</field>
      <field name="inherit_id" ref="crm.crm_lead_view_form"/>
      <field name="arch" type="xml">
        <xpath expr="//field[@name='partner_name']" position="replace"/>
        <xpath expr="//field[@name='function']" position="replace"/>
        <xpath expr="//field[@name='partner_id']" position="replace"/>
        <xpath expr="//group[@name='lead_partner']" position="replace">
          <group col="16" class="col-lg-8">
            <separator string="Mudanza" colspan="16"/>
            <group col="6" colspan="9" class="w-100">
              <div style="display: flex; flex-direction: row;">
                <div colspan="2" style="display: flex; flex-direction: column; padding-right: 20px; width: 13%;">
                  <label for="cantidad" style="font-weight: bold; margin-bottom: 4px;">Cantidad</label>
                  <field name="cantidad" style="border: 1px solid #999; background-color: rgba(0, 0, 0, 0.08)"/>
                </div>
                <div colspan="6" style="display: flex; flex-direction: column; padding-right: 20px;">
                  <label for="objeto" style="font-weight: bold; margin-bottom: 4px;">Objeto</label>
                  <field name="objeto" style="border: 1px solid #999; background-color: rgba(0, 0, 0, 0.08)"/>
                </div>
                <div colspan="2" style="display: flex; flex-direction: column; padding-right: 20px; width: 13%;">
                  <label for="peso" style="font-weight: bold; margin-bottom: 4px;">Kg Aprox.</label>
                  <field name="peso" style="border: 1px solid #999; background-color: rgba(0, 0, 0, 0.08)"/>
                </div>
                <div colspan="1" style="display: flex; flex-direction: column; text-align: center; padding-right: 10px;">
                  <label for="embalaje" style="font-weight: bold; margin-bottom: 4px;">Embalaje</label>
                  <field name="embalaje"/>
                </div>
                <div colspan="1" style="display: flex; flex-direction: column; text-align: center; padding-right: 10px; width: 30%;">
                  <label for="desmontaje" style="font-weight: bold; margin-bottom: 4px;">Desmontaje</label>
                  <field name="desmontaje"/>
                </div>
                <div colspan="6" style="display: flex; flex-direction: column;">
                  <label for="habitacion" style="font-weight: bold; margin-bottom: 4px;">Habitaci√≥n</label>
                  <field name="habitacion" style="border: 1px solid #999; background-color: rgba(0, 0, 0, 0.08)"/>
                </div>
              </div>
            </group>
            <div colspan="14">
              <field name="observaciones_mudanza" style="border: 1px solid #999; background-color: rgba(0, 0, 0, 0.08)"/>
            </div>
          </group>

          <div class="col-lg-4" style="flex: 0 0 33.333%; max-width: 33.333%;">
            <div class="row">
              <div class="w-100">
                <group string="Oferta">
                  <field name="precio_oferta" style="border: 1px solid #999; background-color: rgba(0, 0, 0, 0.08); padding: 4px;"/>
                  <field name="tipo_oferta" style="border: 1px solid #999; background-color: rgba(0, 0, 0, 0.08); padding: 4px;"/>
                </group>
              </div>
            </div>
          </div>

          <div class="row pr-0 mr-0 pr-0">
            <div class="col-10 mr-0 pr-0 w-100">
              <group string="Carga" col="4">
                <field name="streetup" placeholder="Calle..." class="o_address_street" style="border: 1px solid #999; background-color: rgba(0, 0, 0, 0.08); padding: 4px;" colspan="3"/>
                <i class="fa fa-arrow-right" role="img" aria-label="De carga a descarga" colspan="1"/>
                <field name="streetup2" placeholder="Calle 2..." class="o_address_street" style="border: 1px solid #999; background-color: rgba(0, 0, 0, 0.08); padding: 4px;" colspan="3"/>
                <i class="fa fa-arrow-right" role="img" aria-label="De carga a descarga" colspan="1"/>
                <field name="zipup" placeholder="C.P." class="o_address_zip" style="border: 1px solid #999; background-color: rgba(0, 0, 0, 0.08); padding: 4px;" colspan="3"/>
                <i class="fa fa-arrow-right" role="img" aria-label="De carga a descarga" colspan="1"/>
                <field name="cityup" placeholder="Ciudad" class="o_address_city" style="border: 1px solid #999; background-color: rgba(0, 0, 0, 0.08); padding: 4px;" colspan="3"/>
                <i class="fa fa-arrow-right" role="img" aria-label="De carga a descarga" colspan="1"/>
                <field name="state_up" class="o_address_state" placeholder="Estado" style="border: 1px solid #999; background-color: rgba(0, 0, 0, 0.08); padding: 4px;" options="{'no_open': True}" colspan="3"/>
                <i class="fa fa-arrow-right" role="img" aria-label="De carga a descarga" colspan="1"/>
                <field name="province_up" class="o_address_state" placeholder="Provincia" style="border: 1px solid #999; background-color: rgba(0, 0, 0, 0.08); padding: 4px;" options="{'no_open': True}" widget="mudanzas_province" colspan="3"/>
                <i class="fa fa-arrow-right" role="img" aria-label="De carga a descarga" colspan="1"/>
              </group>
            </div>
          </div>

          <div class="row mr-0 pr-0">
            <div class="col-9 w-100">
              <group string="Descarga" col="4">
                <field name="streetdown" placeholder="Calle..." class="o_address_street" style="border: 1px solid #999; background-color: rgba(0, 0, 0, 0.08); padding: 4px;" colspan="4"/>
                <field name="streetdown2" placeholder="Calle 2..." class="o_address_street" style="border: 1px solid #999; background-color: rgba(0, 0, 0, 0.08); padding: 4px;" colspan="4"/>
                <field name="zipdown" placeholder="C.P." class="o_address_zip" style="border: 1px solid #999; background-color: rgba(0, 0, 0, 0.08); padding: 4px;" colspan="4"/>
                <field name="citydown" placeholder="Ciudad" class="o_address_city" style="border: 1px solid #999; background-color: rgba(0, 0, 0, 0.08); padding: 4px;" colspan="4"/>
                <field name="state_down" class="o_address_state" placeholder="Estado" style="border: 1px solid #999; background-color: rgba(0, 0, 0, 0.08); padding: 4px;" options="{'no_open': True}" colspan="4"/>
                <field name="province_down" class="o_address_state" placeholder="Provincia" style="border: 1px solid #999; background-color: rgba(0, 0, 0, 0.08); padding: 4px;" options="{'no_open': True}" widget="mudanzas_province" colspan="4"/>
              </group>
            </div>
          </div>

          <separator style="border-top: 2px solid black;"/>

        </xpath>
      </field>
    </record>
  </data>
</odoo>